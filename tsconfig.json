{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@theme/*": ["src/theme/*"],
      "@icons/*": ["src/icons/*"],
      "@stores/*": ["src/stores/*"],
      "@utils/*": ["src/utils/*"],
      "@db/*": ["src/db/*"],
      "@landing-projects/*": ["src/landing-projects/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
